  <section style="" class="track row">
    <div class="track-box col-md-4">
      <form class="track-box-form track-form" id="track-form" onsubmit="event.preventDefault(); myFunction();">
        <div class="row">
          <div class="col-md-6">
            <div class="form__group">
              <input type="number" class="form__input form__input--track  form__input--track-fat " placeholder="Age" id="age" required>
              <label for="age" class="form__label form__label--track">Age</label>
            </div>
            <div class="form__group">
              <input type="text" class="form__input form__input--track  form__input--track-bmi" placeholder="Gender M/F" id="gender" required>
              <label for="gender" class="form__label form__label--track">Gender M/F</label>
            </div>
            <div class="form__group">
              <input type="text" class="form__input form__input--track  form__input--track-weight" placeholder="Do You Smoke Y/N" id="smoke" required>
              <label for="smoke" class="form__label form__label--track">Do You Smoke Y/N</label>
            </div>
            <div class="form__group">
              <input type="number" class="form__input form__input--track  form__input--track-sugar" placeholder="Cholesterol" id="chol" required>
              <label for="chol" class="form__label form__label--track">Cholesterol</label>
            </div>
            <div class="form__group">
              <input type="number" class="form__input form__input--track  form__input--track-bp" placeholder="Blood Pressure High" id="bpH" required>
              <label for="bpH" class="form__label form__label--track">Blood Pressure High</label>
            </div>
            <div class="form__group">
              <input style="background-color: #00e676; color: white;" type="submit" class="form__input form__input--track form__group form__group--btn btn btn--green" value="Submit" placeholder="Submit" id="sub" required>
            </div>
          </div>
          <div class="col-md-6">
            <div class="form__group">
              <input type="number" class="form__input form__input--track  form__input--track-fat " placeholder="Glucose Level" id="gluc" required>
              <label for="gluc" class="form__label form__label--track">Glucose Level</label>
            </div>
            <div class="form__group">
              <input type="text" class="form__input form__input--track  form__input--track-bmi" placeholder="Diabates Y/N" id="diab" required>
              <label for="diab" class="form__label form__label--track">Diabates Y/N</label>
            </div>
            <div class="form__group">
              <input type="number" class="form__input form__input--track  form__input--track-weight" placeholder="Resting Heart Rate" id="Hrate" required>
              <label for="Hrate" class="form__label form__label--track">Resting Heart Rate</label>
            </div>
            <div class="form__group">
              <input type="text" class="form__input form__input--track  form__input--track-sugar" placeholder="Do You Drink Y/N" id="alco" required>
              <label for="alco" class="form__label form__label--track">Do You Drink Y/N</label>
            </div>
            <div class="form__group">
              <input type="number" class="form__input form__input--track  form__input--track-sugar" placeholder="Blood Pressure Low" id="bpL" required>
              <label for="bpL" class="form__label form__label--track">Blood Pressure Low</label>
            </div>
          </div>
        </div>
      </form>
    </div>
