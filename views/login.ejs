<div class="loginBox form u-margin-bottom-big">
  <div class="form__box bgCol">
    <div class="form__heading u-margin-bottom-medium">
      <h3 class="heading-primary--colored">
        Login
      </h3>
    </div>


    <form action="/api/user/login" method="POST">
      <div class="form__group">
        <input type="email" class="form__input" placeholder="Email Address" id="email" name="email" required>
        <label for="email" class="form__label">Email</label>
      </div>

      <div class="form__group">
        <input type="password" class="form__input" id="password" placeholder="Password" name="password" required>
        <label for="password" class="form__label">Password</label>
      </div>

      <div class="form__register">
        <a href="/signup">Register with us</a>
      </div>



      <div class="form__group form__group--btn">
        <button class="btn btn--green">Log In</button>
      </div>
    </form>


  </div>
